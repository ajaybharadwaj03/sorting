public class Main
{
	public static void main(String[] args) {
	    int[] nums={7,6,9,4,5,25};
	    int i,j;
	    int n= nums.length,temp;
	    for(i=0;i<n;i++){
	        for(j=i+1;j<n;j++){
	            if(nums[i]>nums[j]){
	                temp=nums[i];
	                nums[i]=nums[j];
	                nums[j]=temp;
	            }
	        }
	    }
	    for(i=0;i<n;i++){
	        System.out.print(nums[i]+" ");
	    }
	}
}
